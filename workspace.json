{
  "version": 2,
  "projects": {
    "shared-utils": {
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shared/utils",
            "main": "libs/shared/utils/src/index.ts",
            "tsConfig": "libs/shared/utils/tsconfig.lib.json",
            "assets": ["libs/shared/utils/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/utils"],
          "options": {
            "jestConfig": "libs/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shop": {
      "root": "apps/shop",
      "sourceRoot": "apps/shop",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/shop",
            "outputPath": "dist/apps/shop",
            "buildLibsFromSource": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "shop:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "shop:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "shop:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/shop"],
          "options": {
            "jestConfig": "apps/shop/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shop/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "shop-e2e": {
      "root": "apps/shop-e2e",
      "sourceRoot": "apps/shop-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/shop-e2e/cypress.json",
            "tsConfig": "apps/shop-e2e/tsconfig.e2e.json",
            "devServerTarget": "shop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shop:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shop-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shop-feature-about": {
      "root": "libs/shop/feature-about",
      "sourceRoot": "libs/shop/feature-about/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shop/feature-about",
            "tsConfig": "libs/shop/feature-about/tsconfig.lib.json",
            "project": "libs/shop/feature-about/package.json",
            "entryFile": "libs/shop/feature-about/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/shop/feature-about/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shop/feature-about/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shop/feature-about"],
          "options": {
            "jestConfig": "libs/shop/feature-about/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "shop"
}
